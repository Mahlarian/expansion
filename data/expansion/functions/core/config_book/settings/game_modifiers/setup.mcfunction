function expansion:core/config_book/clear_hotbar
replaceitem entity @s hotbar.8 barrier{display:{Name:'{"text":"Return to Main Menu","color":"red","bold":true,"italic":true}',Lore:['{"text":"Dropping this item will return back to the config main menu","color":"white","italic":false}']}} 1
replaceitem entity @s hotbar.0 nether_star{display:{Name:'{"text":"Change spawn behavior","color":"light_purple","bold":true,"italic":true}',Lore:['{"text":"Dropping this item allows you to modify","color":"white","italic":true}','{"text":"how the spawning conditions will work","color":"white","italic":true}','{"text":"when the game starts.","color":"white","italic":true}']}} 1
replaceitem entity @s hotbar.1 poppy{display:{Name:'{"text":"Grace Mode","color":"aqua","bold":true,"italic":true}',Lore:['{"text":"Dropping this item allows you to enable/disable","color":"white","italic":true}','{"text":"grace period","color":"white","italic":true}','{"text":" "}','{"text":"Grace period is a period of time given","color":"white","italic":true}','{"text":"at the start of the game where the timer","color":"white","italic":true}','{"text":"will not count down. Amount varies by difficulty","color":"white","italic":true}']}} 1
kill @e[type=item,distance=..2,nbt={Item:{id:"minecraft:diamond",Count:1b,tag:{display:{Name:'{"text":"Modifiers","color":"aqua","bold":true,"italic":true}',Lore:['{"text":"Dropping this item will open the modifiers menu","color":"white","italic":true}','{"text":"Modifiers are methods of changing","color":"white","italic":true}','{"text":"the game and adding replayability.","color":"white","italic":true}','{"text":"this can range from small changes to","color":"white","italic":true}','{"text":"entire new gamemodes","color":"white","italic":true}']}}}}]